{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"quotes": "off",
		"require-jsdoc": [
			2,
			{
				"require": {
					"FunctionDeclaration": true,
					"MethodDefinition": false,
					"ClassDeclaration": false
				}
			}
		]
	},
	"extends": [
		"eslint:recommended",
		"google"
	]
}